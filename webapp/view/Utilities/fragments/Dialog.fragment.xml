<core:FragmentDefinition
	xmlns="sap.m"
	xmlns:core="sap.ui.core">
	<SelectDialog
		title="Centros"
		class="sapUiPopupWithPadding"
		items="{/CENTRO}"
		search="_handleValueHelpSearch"
		confirm="_handleValueHelpClose"
		cancel="_handleValueHelpClose"
		multiSelect="true">
		<StandardListItem
			iconDensityAware="false"
			iconInset="false"
			title="{CENTRO}"
			description="{Ce.}" />
	</SelectDialog>
</core:FragmentDefinition>