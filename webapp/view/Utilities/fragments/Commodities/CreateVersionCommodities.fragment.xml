<c:FragmentDefinition xmlns:m="sap.m" xmlns:f="sap.ui.layout.form" xmlns:c="sap.ui.core" xmlns:mvc="sap.ui.core.mvc"
	xmlns:html="http://www.w3.org/1999/xhtml" xmlns:l="sap.ui.layout">
	<m:Dialog title="Crear version commoditie">
		<m:VBox class="sapUiSmallMargin">
			<f:SimpleForm columnsM="2" columnsL="2" columnsXL="2">
				<f:content>
					<m:Label text="Nombre"/>
					<m:Input name="txtName"/>
					<m:Label text="Descripcion"/>
					<m:Input name="txtDetail"/>
					<m:Label text="Commoditie"/>
					<m:ComboBox id="cbxCommodities" showSecondaryValues="true">
						<c:ListItem text="Commoditie 1" key="0" additionalText="1"/>
						<c:ListItem text="Commoditie 2" key="1" additionalText="2"/>
						<c:ListItem text="Commoditie 3" key="2" additionalText="3"/>
					</m:ComboBox>
					<m:Label text="Periodo"/>
					<m:ComboBox id="cbxPeriodo" showSecondaryValues="true">
						<c:ListItem text="2020" key="0" additionalText="2020"/>
						<c:ListItem text="2021" key="1" additionalText="2020"/>
						<c:ListItem text="2022" key="2" additionalText="2022"/>
						<c:ListItem text="2023" key="3" additionalText="2023"/>
						<c:ListItem text="2024" key="4" additionalText="2024"/>
						<c:ListItem text="2025" key="5" additionalText="2025"/>
					</m:ComboBox>
				</f:content>
			</f:SimpleForm>
		</m:VBox>
		<m:beginButton>
			<m:Button icon="sap-icon://save" text="save" tooltip="save" press="SaveVersionCommoditie"/>
		</m:beginButton>
		<m:endButton>
			<m:Button icon="sap-icon://decline" text="cancel" tooltip="cancel" press="closeDialog"/>
		</m:endButton>
	</m:Dialog>
</c:FragmentDefinition>