<mvc:View
	controllerName="cbc.co.simulador_costos.controller.Manufactura.Manufactura"
	height="100%"
	xmlns:l="sap.ui.layout"
	xmlns:mvc="sap.ui.core.mvc"
	xmlns:core="sap.ui.core"
	xmlns="sap.m">
		<Toolbar class="sapNocolor">
			<Title text="Carga de Manufactura" level="H2"/>
			<ToolbarSpacer/>
			<Title text="" level="H2"/>
			<ToolbarSpacer/>
			<Button icon="sap-icon://message-information" press=".onOpenDialog" ariaLabelledBy="editButtonLabel"/>
		</Toolbar>
		<IconTabBar
		id="idIconTabBarSeparatorNoIcon"
		expanded="true"
		class="sapUiResponsiveContentPadding">
		<items>
			<IconTabFilter
				icon="sap-icon://upload"
				iconColor="Neutral"
				text="Cargar">
			</IconTabFilter>
			<IconTabFilter
				icon="sap-icon://edit"
				iconColor="Neutral"
				text="Editar">
				<mvc:XMLView viewName="cbc.co.simulador_costos.view.InvoiceList"/>
			</IconTabFilter>
		</items>
	</IconTabBar>
	
</mvc:View>