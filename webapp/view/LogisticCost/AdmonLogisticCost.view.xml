<mvc:View controllerName="cbc.co.simulador_costos.controller.LogisticCost.AdmonLogisticCost" height="100%" xmlns:l="sap.ui.layout"
	xmlns:mvc="sap.ui.core.mvc" xmlns:core="sap.ui.core" xmlns:m="sap.m" xmlns="sap.ui.table">
	<m:App>
		<m:pages>
			<m:Page title="{i18n>CostosLogisticos}" showNavButton="true" navButtonPress="onNavBack">
				<m:content>
					<m:FlexBox height="auto" alignItems="Start" justifyContent="Start">
						<m:items>
							<m:Button icon="sap-icon://copy" text="Copiar" tooltip="Copiar version de commoditie" press="showFormCopyVersionCommoditie"/>
							<m:Button icon="sap-icon://save" text="Guardar" tooltip="Guardar version de commoditie" press="saveVersionCommodities"/>
							<m:Button icon="sap-icon://upload-to-cloud" text="Publicar" tooltip="Publicar version de commoditie" press="PublicVersionCommodities"/>
						</m:items>
					</m:FlexBox>
					<Table id="tblLogicCost" selectionMode="Single" visibleRowCount="7" rowActionCount="1" class="sapUiSizeCompact">
						<extension>
							<m:OverflowToolbar>
								<m:content>
									<m:Title id="title" text="Commodities"/>
									<m:ToolbarSpacer/>
									<m:Button icon="sap-icon://sys-next-page" text="Exportar" tooltip="Exportar version de commoditie"/>
									<m:Button icon="sap-icon://sys-prev-page" text="Importar" tooltip="Importar version de commoditie"/>
									<m:Button icon="sap-icon://factory" type="Emphasized" text="Administrar Commodities" tooltip="Crear Commodities"
										press="onGoToIdCommoditieTable"/>
									<m:Button icon="sap-icon://edit" type="Emphasized" text="Editar Version Default" tooltip="Version Default" press="showFormEditCommoditie"/>
								</m:content>
							</m:OverflowToolbar>
						</extension>
						<columns>
							<Column>
								<m:Label text="Pais"/>
								<template>
									<m:Text text="" wrapping="false"/>
								</template>
							</Column>
							<Column>
								<m:Label text="Nombre Versión"/>
								<template>
									<m:Text text="" wrapping="false"/>
								</template>
							</Column>
							<Column>
								<m:Label text="Descripción Versión"/>
								<template>
									<m:Text text="" wrapping="false"/>
								</template>
							</Column>
							<Column>
								<m:Label text="Versión Materiales"/>
								<template>
									<m:Text text="" wrapping="false"/>
								</template>
							</Column>
							<Column>
								<m:Label text="Origen"/>
								<template>
									<m:Text text="" wrapping="false"/>
								</template>
							</Column>
							<Column>
								<m:Label text="Año/Mes"/>
								<template>
									<m:Text text="" wrapping="false"/>
								</template>
							</Column>
							<Column>
								<m:Label text="ID Versión"/>
								<template>
									<m:Text text="" wrapping="false"/>
								</template>
							</Column>
						</columns>
						<rowActionTemplate>
							<RowAction>
								<items>
									<RowActionItem type="Navigation" text="Editar" press="handleEditPress" icon="sap-icon://edit"></RowActionItem>
								</items>
							</RowAction>
						</rowActionTemplate>
						<!--<rowSettingsTemplate>-->
						<!--	<RowSettings highlight="None"/>-->
						<!--</rowSettingsTemplate>-->
					</Table>
				</m:content>
			</m:Page>
		</m:pages>
	</m:App>
</mvc:View>