<mvc:View
	controllerName="cbc.co.simulador_costos.controller.DataDefault.Icoterm.GridIcoterm"
	xmlns:html="http://www.w3.org/1999/xhtml" 
	xmlns:mvc="sap.ui.core.mvc" 
	displayBlock="true"
	xmlns="sap.ui.table" 
	xmlns:m="sap.m" 
	xmlns:u="sap.ui.unified" 
	xmlns:c="sap.ui.core"
    height="100%">
	<m:ObjectHeader binding="{/ProductCollection/0}" title="Datos Default - Icoterm" condensed="true"
		number="{ parts:[{path:'Price'},{path:'CurrencyCode'}], type: 'sap.ui.model.type.Currency', formatOptions: {showMeasure: false} }"
		numberUnit="{CurrencyCode}">
		<m:ObjectAttribute text="{WeightMeasure} {WeightUnit} {Width} x {Depth} x {Height} {DimUnit}"/>
	</m:ObjectHeader>
	 <m:App>
	   <m:pages>
		<m:Page
			showHeader="false"
			enableScrolling="false"
			class="sapUiContentPadding">
			<m:content>
				<Table id="tblIcoterm"
					rows="{/ICOTERM}"
					selectionMode="None"
					visibleRowCount="12">
					<extension>
							<m:OverflowToolbar>
								<m:content>
									<m:Title id="title" text="Icoterm"/>
									<m:ToolbarSpacer/>
									<m:Button icon="sap-icon://sys-next-page" text="Exportar" tooltip="Exportar Icoterm"/>
									<m:Button icon="sap-icon://sys-prev-page" text="Importar" tooltip="Importar Icoterm"/>
									<m:Button icon="sap-icon://factory" text="Crear Icoterm" tooltip="Crear Icoterm" press="showFormAddCommoditie"/>
									<m:Button icon="sap-icon://save" text="Guardar" tooltip="Guardar" press="saveVersionCommodities"/>
								</m:content>
							</m:OverflowToolbar>
						</extension>
					<columns>
						<Column width="15%">
							<m:Label text="ID" />
							<template>
								<m:Text text="{ID}" wrapping="false" />
							</template>
						</Column>
						<Column
							id="ColumnICOTERM"
							width="15%"
							showSortMenuEntry="false"
							sortProperty="ICOTERM">
							<m:Label text="DescripciÃ³n" />
							<template>
								<m:Text text="{ICOTERM}" wrapping="false" />
							</template>
						</Column>
					</columns>
				</Table>
			</m:content>
		</m:Page>
	  </m:pages>
	 </m:App>
</mvc:View>