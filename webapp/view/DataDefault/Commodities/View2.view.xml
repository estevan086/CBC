<mvc:View xmlns="sap.m" xmlns:layout="sap.ui.layout" xmlns:mvc="sap.ui.core.mvc" xmlns:ui="sap.ui.table"
	controllerName="sap.ui.demo.walkthrough.controller.DataDefault.Commodities.View2">
	<App>
		<pages>
			<Page showHeader="false" enableScrolling="false" class="sapUiContentPadding" showNavButton="true" navButtonPress="onBack">
				<content>
					<Button text="To the first Page" press="onToPage1"/>
					<ui:Table id="tblCommodities" rows="{/COMMODITIES}" selectionMode="None" visibleRowCount="10">
						<ui:columns>
							<ui:Column>
								<Label text="ICDEF_IDCOMMODITIES"/>
								<ui:template>
									<Text text="{CDEF_IDCOMMODITIES}" wrapping="false"/>
								</ui:template>
							</ui:Column>
							<ui:Column>
								<Label text="CDEF_COMMODITIE"/>
								<ui:template>
								</ui:template>
							</ui:Column>
							<ui:Column>
								<Label text="CDEF_SOCIEDAD"/>
								<ui:template>
									<Text text="{CDEF_SOCIEDAD}" wrapping="false"/>
								</ui:template>
							</ui:Column>
							<ui:Column>
								<Label text="CDEF_MONEDA"/>
								<ui:template>
									<Text text="{CDEF_MONEDA}" wrapping="false"/>
								</ui:template>
							</ui:Column>
							<ui:Column>
								<Label text="CDEF_UMD"/>
								<ui:template>
									<Text text="{CDEF_UMD}" wrapping="false"/>
								</ui:template>
							</ui:Column>
							<ui:Column>
								<Label text="CDEF_PRECIO"/>
								<ui:template>
									<Text text="{CDEF_PRECIO}" wrapping="false"/>
								</ui:template>
							</ui:Column>
							<ui:Column>
								<Label text="CDEF_OTROCOSTO"/>
								<ui:template>
									<Text text="{CDEF_OTROCOSTO}" wrapping="false"/>
								</ui:template>
							</ui:Column>
							<ui:Column>
								<Label text="CDEF_FORMULA"/>
								<ui:template>
									<Text text="{CDEF_FORMULA}" wrapping="false"/>
								</ui:template>
							</ui:Column>
							<ui:Column>
								<Label text="CDEF_PERIODO"/>
								<ui:template>
									<Text text="{CDEF_PERIODO}" wrapping="false"/>
								</ui:template>
							</ui:Column>
							<ui:Column>
								<Label text="CDEF_MES"/>
								<ui:template>
									<Text text="{CDEF_MES}" wrapping="false"/>
								</ui:template>
							</ui:Column>
						</ui:columns>
					</ui:Table>
				</content>
			</Page>
		</pages>
	</App>
</mvc:View>