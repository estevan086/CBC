<mvc:View
	controllerName="cbc.co.simulador_costos.controller.Admon.CreateParameters"
	height="100%"
	xmlns:l="sap.ui.layout"
	xmlns:mvc="sap.ui.core.mvc"
	xmlns:core="sap.ui.core"
	xmlns="sap.m">
		<Toolbar class="sapNocolor">
			<Title text="Creación de Parametros" level="H2"/>
			<ToolbarSpacer/>
			<Title text="" level="H2"/>
			<ToolbarSpacer/>
			<Button icon="sap-icon://message-information" press=".onOpenDialog" ariaLabelledBy="editButtonLabel"/>
		</Toolbar>
		<IconTabBar
		id="idIconTabBarSeparatorNoIcon"
		expanded="true"
		class="sapUiResponsiveContentPadding">
		<items>
			<IconTabFilter
				icon="sap-icon://copy"
				iconColor="Neutral"
				text="Maestros SAP">
				<mvc:XMLView viewName="cbc.co.simulador_costos.view.Admon.CreateParameters.MasterSap"/>
			</IconTabFilter>
			<IconTabFilter
				icon="sap-icon://date-time"
				iconColor="Neutral"
				text="Tiempo">
				<!--<mvc:XMLView viewName="cbc.co.simulador_costos.view.Admon.Periodo"/>-->
			</IconTabFilter>
			<IconTabFilter
				icon="sap-icon://travel-expense"
				iconColor="Neutral" text="Logística">
				<mvc:XMLView viewName="cbc.co.simulador_costos.view.Admon.CreateParameters.Logistica"/>
			</IconTabFilter>
		</items>
	</IconTabBar>
</mvc:View>