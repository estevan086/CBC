{
	"_version": "1.17.0",
	"sap.app": {
		"id": "cbc.co.simulador_costos",
		"type": "application",
		"i18n": "i18n/i18n.properties",
		"title": "{{appTitle}}",
		"description": "{{appDescription}}",
		"applicationVersion": {
			"version": "1.0.0"
		},
		"dataSources": {
			"Z_SIMULADOR_SRV": {
				"uri": "/sap/opu/odata/sap/Z_SIMULADOR_SRV/",
				"type": "OData",
				"settings": {
					"localUri": "localService/metadata.xml"
				}
			}
		},
		"sourceTemplate": {
			"id": "servicecatalog.connectivityComponentForManifest",
			"version": "0.0.0"
		}
	},
	"sap.ui": {
		"technology": "UI5",
		"deviceTypes": {
			"desktop": true,
			"tablet": true,
			"phone": true
		},
		"fullWidth": true,
		"flexEnabled": false,
		"supportedThemes": [
			"sap_belize_hcb",
			"sap_belize_hcw",
			"sap_belize",
			"sap_belize_plus",
			"sap_fiori_3",
			"sap_fiori_3_hcb",
			"sap_fiori_3_hcw"
		]
	},
	"sap.ui5": {
		"rootView": {
			"viewName": "cbc.co.simulador_costos.view.App",
			"type": "XML",
			"async": true,
			"id": ""
		},
		"dependencies": {
			"minUI5Version": "1.62.0",
			"libs": {
				"sap.m": {},
				"sap.ui.core": {},
				"sap.ui.layout": {}
			}
		},
		"models": {
			"i18n": {
				"type": "sap.ui.model.resource.ResourceModel",
				"settings": {
					"bundleName": "cbc.co.simulador_costos.i18n.i18n"
				}
			},
			"invoice": {
				"type": "sap.ui.model.json.JSONModel",
				"uri": "Invoices.json"
			},
			"ModelSimulador": {
				"type": "sap.ui.model.odata.v2.ODataModel",
				"settings": {
					"defaultOperationMode": "Server",
					"defaultBindingMode": "OneWay",
					"defaultCountMode": "Request"
				},
				"dataSource": "Z_SIMULADOR_SRV",
				"preload": true
			}
		},
		"resources": {
			"css": [
				{
					"uri": "css/style.css"
				}
			]
		},
		"contentDensities": {
			"compact": true,
			"cozy": true
		},
		"flexEnabled": false,
		"routing": {
			"routes": {
				"home": {
					"pattern": "",
					"target": "parentTarget"
				},
				"rtChCommodities": {
					"pattern": "rtChCommodities",
					"target": "rtChCommoditiesTarget"
				},
				"rtChFromuladora": {
					"pattern": "rtChFromuladora/{oRowPath}",
					"target": "rtChFromuladoraTarget"
				},
				"rtChIDCommodities": {
					"pattern": "rtChIDCommodities",
					"target": "rtChIDCommoditiesTarget"
				},
				"rtChMateriales": {
					"pattern": "rtChMateriales",
					"target": "rtChMaterialesTarget"
				},
				"rtChMantenimientoCommodities": {
					"pattern": "rtChMantenimientoCommodities",
					"target": "rtChMantenimientoCommoditiesTarget"
				},
				"rtChCostosLogisticos": {
					"pattern": "rtChCostosLogisticos",
					"target": "rtChCostosLogisticosTarget"
				},
				"rtChUsers": {
					"pattern": "rtChUsers",
					"target": "rtChUsersTarget"
				}
			},
			"targets": {
				"parentTarget": {
					"viewName": "ShellBarWithSplitApp",
					"viewLevel": 1
				},
				"childTarget0": {
					"viewPath": "demo.view.children",
					"viewName": "Empty",
					"parent": "parentTarget",
					"controlId": "parentNavContainer",
					"controlAggregation": "pages",
					"transition": "flip",
					"viewLevel": 2
				},
				"rtChCommoditiesTarget": {
					"viewPath": "cbc.co.simulador_costos.view.DataDefault.Commodities",
					"viewName": "AdminCommodities",
					"parent": "parentTarget",
					"controlId": "parentNavContainer",
					"controlAggregation": "pages",
					"transition": "slide",
					"viewLevel": 3
				},
				"rtChFromuladoraTarget": {
					"viewPath": "cbc.co.simulador_costos.view.Utilities",
					"viewName": "Formuladora",
					"parent": "parentTarget",
					"controlId": "parentNavContainer",
					"transition": "flip",
					"controlAggregation": "pages",
					"viewLevel": 4
				},
				"rtChIDCommoditiesTarget": {
					"viewPath": "cbc.co.simulador_costos.view.DataDefault.Commodities",
					"viewName": "AdminIDCommodities",
					"parent": "parentTarget",
					"controlId": "parentNavContainer",
					"transition": "flip",
					"controlAggregation": "pages",
					"viewLevel": 4
				},
				"rtChMaterialesTarget": {
					"viewPath": "cbc.co.simulador_costos.view.DataDefault.Materiales",
					"viewName": "AdminMateriales",
					"parent": "parentTarget",
					"controlId": "parentNavContainer",
					"transition": "slide",
					"controlAggregation": "pages",
					"viewLevel": 3
				},
				"rtChMantenimientoCommoditiesTarget": {
					"viewPath": "cbc.co.simulador_costos.view.Commodities",
					"viewName": "Commodities",
					"parent": "parentTarget",
					"controlId": "parentNavContainer",
					"transition": "slide",
					"controlAggregation": "pages",
					"viewLevel": 3
				},
				"rtChCostosLogisticosTarget": {
					"viewPath": "cbc.co.simulador_costos.view.DataDefault.LogisticCost",
					"viewName": "AdminLogisticCost",
					"parent": "parentTarget",
					"controlId": "parentNavContainer",
					"transition": "slide",
					"controlAggregation": "pages",
					"viewLevel": 3
				},
				"rtChUsersTarget": {
					"viewPath": "cbc.co.simulador_costos.view.Admon",
					"viewName": "Users",
					"parent": "parentTarget",
					"controlId": "parentNavContainer",
					"transition": "slide",
					"controlAggregation": "pages",
					"viewLevel": 3
				},
				"notFound": {
					"viewName": "Parent",
					"transition": "flip",
					"viewLevel": 0
				}
			},
			"config": {
				"viewPath": "cbc.co.simulador_costos.view",
				"controlId": "myApp",
				"controlAggregation": "pages",
				"routerClass": "sap.m.routing.Router",
				"fullWidth": true,
				"viewType": "XML",
				"async": true,
				"bypassed": {
					"target": "notFound"
				}
			}
		}
	},
	"sap.platform.abap": {
		"uri": "/sap/bc/ui5_ui5/sap/zsimulador_001/webapp",
		"_version": "1.1.0"
	}
}