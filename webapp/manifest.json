{
	"_version": "1.17.0",
	"sap.app": {
		"id": "cbc.co.simulador_costos",
		"type": "application",
		"i18n": "i18n/i18n.properties",
		"title": "{{appTitle}}",
		"description": "{{appDescription}}",
		"applicationVersion": {
			"version": "1.0.0"
		},
		"dataSources": {
			"Z_SIMULADOR_SRV": {
				"uri": "/sap/opu/odata/sap/Z_SIMULADOR_SRV/",
				"type": "OData",
				"settings": {
					"localUri": "localService/metadata.xml"
				}
			}
		},
		"sourceTemplate": {
			"id": "servicecatalog.connectivityComponentForManifest",
			"version": "0.0.0"
		}
	},
	"sap.ui": {
		"technology": "UI5",
		"deviceTypes": {
			"desktop": true,
			"tablet": true,
			"phone": true
		},
		"fullWidth": true,
		"flexEnabled": false,
		"supportedThemes": [
			"sap_belize_hcb",
			"sap_belize_hcw",
			"sap_belize",
			"sap_belize_plus",
			"sap_fiori_3",
			"sap_fiori_3_hcb",
			"sap_fiori_3_hcw"
		]
	},
	"sap.ui5": {
		"rootView": {
			"viewName": "cbc.co.simulador_costos.view.App",
			"type": "XML",
			"async": true,
			"id": ""
		},
		"dependencies": {
			"minUI5Version": "1.62.0",
			"libs": {
				"sap.m": {},
				"sap.ui.core": {},
				"sap.ui.layout": {}
			}
		},
		"models": {
			"i18n": {
				"type": "sap.ui.model.resource.ResourceModel",
				"settings": {
					"bundleName": "cbc.co.simulador_costos.i18n.i18n"
				}
			},
			"invoice": {
				"type": "sap.ui.model.json.JSONModel",
				"uri": "Invoices.json"
			},
			"LogisticCost": {
				"type": "sap.ui.model.json.JSONModel"
			},
			"ModelSimulador": {
				"type": "sap.ui.model.odata.v2.ODataModel",
				"settings": {
					"defaultOperationMode": "Server",
					"defaultBindingMode": "OneWay",
					"defaultCountMode": "Request"
				},
				"dataSource": "Z_SIMULADOR_SRV",
				"preload": true
			},
			"ImgHome": {
				"type": "sap.ui.model.json.JSONModel",
				"settings": {},
				"uri": "model/img.json",
				"preload": true
			},
			"Escenarios": {
				"type": "sap.ui.model.json.JSONModel",
				"settings": {},
				"uri": "model/Escenarios/MatrizEscenarios.json",
				"preload": true
			}
		},
		"resources": {
			"css": [
				{
					"uri": "css/style.css"
				}
			]
		},
		"contentDensities": {
			"compact": true,
			"cozy": true
		},
		"flexEnabled": false,
		"routing": {
			"routes": {
				"home": {
					"pattern": "",
					"target": "parentTarget"
				},
				"rtHomeExpanded": {
					"pattern": "",
					"target": "rtHomeTarget"
				},
				"rtNone": {
					"pattern": "",
					"target": "rtHomeTarget"
				},
				"home2": {
					"pattern": "home",
					"target": "homeTarget"
				},
				"rtHome": {
					"pattern": "rtHome",
					"target": "rtHomeTarget"
				},
				"rtChCommodities": {
					"pattern": "rtChCommodities",
					"target": "rtChCommoditiesTarget"
				},
				"rtChFromuladora": {
					"pattern": "rtChFromuladora/{oIdCommoditie}/oSociedad{oSociedad}/oCentro{oCentro}/oYear{oYear}/oMes{oMes}/oIdFormula{oIdFormula}/oTxt{oTxt}",
					"target": "rtChFromuladoraTarget"
				},
				"rtChIDCommodities": {
					"pattern": "rtChIDCommodities",
					"target": "rtChIDCommoditiesTarget"
				},
				"rtChMateriales": {
					"pattern": "rtChMateriales",
					"target": "rtChMaterialesTarget"
				},
				"rtChMantenimientoCommodities": {
					"pattern": "rtChMantenimientoCommodities",
					"target": "rtChMantenimientoCommoditiesTarget"
				},
				"rtChCostosLogisticos": {
					"pattern": "rtChCostosLogisticos/{version}",
					"target": "rtChCostosLogisticosTarget"
				},
				"rtChCostosLogisticosVersion": {
					"pattern": "rtChCostosLogisticosVersion",
					"target": "rtChCostosLogisticosTarget"
				},
				"rtChProfiles": {
					"pattern": "Profiles",
					"target": "rtChProfilesTarget"
				},
				"rtChCentros": {
					"pattern": "rtChCentros",
					"target": "rtChCentrosTarget"
				},
				"rtChPeriodo": {
					"pattern": "rtChPeriodo",
					"target": "rtChPeriodoTarget"
				},
				"rtChAdminLogistica": {
					"pattern": "rtChAdminLogistica",
					"target": "rtChAdminLogisticaTarget"
				},
				"rtChTypeChange": {
					"pattern": "rtChTypeChange",
					"target": "rtChTypeChangeTarget"
				},
				"rtChCreateIcoterm": {
					"pattern": "rtChCreateIcoterm",
					"target": "rtChCreateIcotermTarget"
				},
				"rtChCopyVersion": {
					"pattern": "rtChCopyVersion",
					"target": "rtHomeTarget"
				},
				"rtChMatrizEs": {
					"pattern": "MatrizEscenarios",
					"target": "rtChMatrizEsTarget"
				},
				"rtChMatrizEsNew": {
					"pattern": "MatrizEscenarios/Nuevo",
					"target": "rtChMatrizEsNewTarget"
				},
				"rtChAdmonEs": {
					"pattern": "rtChAdmonEs",
					"target": "rtHomeTarget"
				},
				"rtChMantenimientoMateriales": {
					"pattern": "rtChMantenimientoMateriales",
					"target": "rtChMantenimientoMaterialesTarget"
				},
				"rtCCOGS": {
					"pattern": "rtCCOGS",
					"target": "rtHomeTarget"
				},
				"rtChVisualizacion": {
					"pattern": "rtChVisualizacion",
					"target": "rtHomeTarget"
				},
				"rtChVolumen": {
					"pattern": "rtChVolumen",
					"target": "rtHomeTarget"
				},
				"rtChAdminCL": {
					"pattern": "rtChAdminCL",
					"target": "rtChAdminCLTarget"
				}
			},
			"targets": {
				"parentTarget": {
					"viewName": "ShellBarWithSplitApp"
				},
				"homeTarget": {
					"viewPath": "cbc.co.simulador_costos.view",
					"viewName": "Home",
					"parent": "parentTarget",
					"controlId": "parentNavContainer",
					"controlAggregation": "pages",
